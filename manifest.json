{
  "name": "tester",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background":{
    "scripts": ["app.js"]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "test",
    "default_icon": "icon.png"
  },
  "permissions": [
    "webRequest",
    "<all_urls>"
  ]
}
